{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "lib": ["es2020"],
    "module": "commonjs",
    "declaration": true,
    "target": "es2020",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true
  },
  "include": ["src"]
}
