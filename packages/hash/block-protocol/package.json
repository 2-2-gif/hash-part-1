{
  "name": "@hashintel/block-protocol",
  "license": "SSPL-1.0",
  "private": true,
  "version": "0.1.1",
  "description": "block-protocol",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "prettier": "@hashintel/prettier-config",
  "scripts": {
    "format": "prettier --write './src/**/*'; eslint --ext .ts --fix ./src",
    "lint": "prettier --check './src/**/*' || exit 1; eslint --ext .ts ./src; tsc --noEmit # @hashintel/block-protocol",
    "build": "yarn clean; tsc -b",
    "clean": "rimraf ./dist/ ./tsconfig.tsbuildinfo",
    "postinstall": "yarn build"
  },
  "devDependencies": {
    "@hashintel/prettier-config": "*",
    "@types/react": "17.0.7",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "prettier": "2.3.2",
    "rimraf": "^3.0.2",
    "typescript": "4.3.5"
  }
}
