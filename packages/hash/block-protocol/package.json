{
  "name": "@hashintel/block-protocol",
  "version": "0.1.1",
  "private": true,
  "description": "block-protocol",
  "license": "SSPL-1.0",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
