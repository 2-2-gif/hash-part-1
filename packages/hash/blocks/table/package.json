{
  "name": "@hashintel/block-table",
  "version": "0.1.0",
  "description": "table block component",
  "private": true,
  "browser": "dist/main.js",
  "author": "Ciaran Morinan",
  "license": "MIT",
  "main": "src/index.ts",
  "prettier": "@hashintel/prettier-config",
  "scripts": {
    "format": "eslint --ext .ts,.tsx --fix ./src/; prettier --write --list-different './src/**/*'",
    "lint": "prettier --check './src/**/*' || exit 1; eslint --ext .ts,.tsx ./src/; tsc --noEmit # @hashintel/block-table",
    "build": "yarn clean; cross-env NODE_ENV=production webpack --mode production && yarn schema",
    "build:dev": "yarn clean; cross-env NODE_ENV=development webpack --mode development",
    "schema": "typescript-json-schema tsconfig.json AppProps --required true --out dist/block-schema.json",
    "type-schema": "node schemaGenerator",
    "webpack-dev-server": "cross-env NODE_ENV=development webpack-dev-server -d --port 9090 --config webpack-dev-server.config.js --open",
    "dev": "yarn start",
    "start": "concurrently -n webpack,webpack-dev-server -c green,cyan \"yarn build:dev -- --watch --verbose\" \"yarn webpack-dev-server\"",
    "clean": "rimraf ./dist/",
    "serve": "serve -C -l 62678 ./dist/"
  },
  "dependencies": {
    "@hashintel/block-protocol": "*",
    "react-table": "7.7.0"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "twind": "0.16.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "7.13.0",
    "@babel/runtime": "^7.12.5",
    "@hashintel/prettier-config": "*",
    "@types/react-dom": "17.0.8",
    "@types/react-table": "7.7.1",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "babel-loader": "^8.2.2",
    "concurrently": "6.2.0",
    "core-js": "^2.6.12",
    "cross-env": "^7.0.3",
    "css-loader": "5.2.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "24.5.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-restricted-imports": "0.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "prettier": "2.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "sass": "1.35.1",
    "sass-loader": "^10",
    "serve": "11.3.2",
    "style-loader": "2.0.0",
    "twind": "0.16.16",
    "typescript": "4.3.5",
    "typescript-json-schema": "0.50.1",
    "webpack": "^4.44.2",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
