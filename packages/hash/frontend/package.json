{
  "name": "@hashintel/hash-frontend",
  "license": "SSPL-1.0",
  "private": true,
  "version": "0.1.0",
  "description": "Frontend for HASH.dev",
  "prettier": "@hashintel/prettier-config",
  "scripts": {
    "format": "prettier --write './src/**/*'; eslint --ext .js,.ts,.tsx --fix ./src/",
    "lint": "prettier --check './src/**/*' || exit 1; eslint --ext .js,.ts,.tsx ./src/; tsc --noEmit # @hashintel/hash-frontend",
    "codegen": "graphql-codegen --config codegen.yml",
    "build": "yarn clean; yarn codegen; next build",
    "clean": "rimraf './src/**/*.gen.*' ./.next/",
    "postinstall": "yarn codegen # required for linting",
    "dev": "next dev",
    "start": "next start"
  },
  "dependencies": {
    "@apollo/client": "3.3.19",
    "@hashintel/block-protocol": "*",
    "@hashintel/hash-shared": "*",
    "@headlessui/react": "1.3.0",
    "@twind/next": "1.0.8",
    "date-fns": "2.22.1",
    "graphql": "15.5.0",
    "jsonschema": "1.4.0",
    "lodash": "4.17.21",
    "next": "10.2.3",
    "next-transpile-modules": "8.0.0",
    "next-images": "1.8.1",
    "prismjs": "1.23.0",
    "prosemirror-collab": "1.2.2",
    "prosemirror-commands": "1.1.10",
    "prosemirror-dev-tools": "3.1.0",
    "prosemirror-dropcursor": "1.3.5",
    "prosemirror-example-setup": "1.1.2",
    "prosemirror-history": "1.2.0",
    "prosemirror-inputrules": "1.1.3",
    "prosemirror-keymap": "1.1.4",
    "prosemirror-model": "1.14.3",
    "prosemirror-schema-basic": "1.1.2",
    "prosemirror-schema-list": "1.1.5",
    "prosemirror-state": "1.3.4",
    "prosemirror-transform": "1.3.2",
    "prosemirror-view": "1.19.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-modal-hook": "3.0.0",
    "setimmediate": "1.0.5",
    "twind": "0.16.16",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-operations": "1.18.0",
    "@hashintel/prettier-config": "*",
    "@types/lodash": "4.14.171",
    "@types/prismjs": "1.16.5",
    "@types/prosemirror-history": "1.0.3",
    "@types/prosemirror-keymap": "1.0.4",
    "@types/prosemirror-model": "1.13.2",
    "@types/prosemirror-transform": "1.1.4",
    "@types/prosemirror-view": "1.18.0",
    "@types/react": "17.0.7",
    "@types/react-dom": "17.0.8",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "babel-plugin-inline-react-svg": "2.0.1",
    "babel-plugin-prismjs": "2.0.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "prettier": "2.3.2",
    "rimraf": "^3.0.2",
    "sass": "1.34.1",
    "typescript": "4.3.5"
  }
}
